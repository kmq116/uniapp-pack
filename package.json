{
  "name": "uniapp-pack",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.14.0",
  "description": "uniapp 打包工具",
  "author": "kmq116 <coderco@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/kmq116",
  "homepage": "https://github.com/kmq116/uniapp-pack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmq116/uniapp-pack.git"
  },
  "bugs": "https://github.com/kmq116/uniapp-pack/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "uniapp-pack": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "test:pack": "node test.js",
    "link:pack": "pnpm link --global",
    "install:pack": "pnpm link uniapp-pack --global",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "inquirer": "^9.1.5",
    "json5": "^2.2.3",
    "zx": "^7.0.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.10.7",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
